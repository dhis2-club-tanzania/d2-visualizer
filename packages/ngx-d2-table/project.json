{
  "projectType": "library",
  "root": "packages/ngx-d2-table",
  "sourceRoot": "packages/ngx-d2-table/src",
  "prefix": "iapps",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/ngx-d2-table",
        "main": "packages/ngx-d2-table/src/index.ts",
        "tsConfig": "packages/ngx-d2-table/tsconfig.lib.json",
        "assets": [
          "packages/ngx-d2-table/*.md",
          {
            "glob": "**/*",
            "input": "node_modules/monaco-editor",
            "output": "assets/monaco-editor"
          }
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/ngx-d2-table"
      ],
      "options": {
        "jestConfig": "packages/ngx-d2-table/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/ngx-d2-table/src/**/*.ts",
          "packages/ngx-d2-table/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
