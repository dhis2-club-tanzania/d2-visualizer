{
  "name": "d2-dashboard",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/d2-dashboard/src",
  "prefix": "d2",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/packages/d2-dashboard"],
      "options": {
        "project": "packages/d2-dashboard/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/d2-dashboard/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/d2-dashboard/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },

    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/d2-dashboard"],
      "options": {
        "jestConfig": "packages/d2-dashboard/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "packages/d2-dashboard/**/*.ts",
          "packages/d2-dashboard/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
